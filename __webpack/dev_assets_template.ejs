{% content_for 'head_top' %}
  <% for (var css in htmlWebpackPlugin.files.css) { %>
  <link href="<%= htmlWebpackPlugin.files.css[css] %>" rel="stylesheet">
  <% } %>

  <link rel="prefetch" as="script" id="main-worker" href="{{ 'autogenerated/sw.js' | asset_url }}">
{% endcontent_for %}

{% content_for 'body_bottom' %}
  <script>
    window.__WEBPACK_ASSETS_PATH__ = function() { return '{{ "" | asset_url }}autogenerated/' };
    window.__ASSETS_PATH__ = '{{ "" | asset_url }}';
  </script>

  <% for (var chunk in htmlWebpackPlugin.files.chunks) { %>
    <% if (chunk !== 'sw') { %>
      <script src="<%= htmlWebpackPlugin.files.chunks[chunk].entry %>" crossorigin="anonymous"></script>
    <% } %>
  <% } %>
{% endcontent_for %}
