{% content_for 'head_top' %}
  {%- include 'layouts/head/prefetch' -%}
  {%- include 'layouts/head/preload' -%}

  <link href="{{ 'autogenerated/common.css' | asset_url }}" rel="stylesheet">
  <link rel="preload" href="{{ 'autogenerated/common.js' | asset_url }}" as="script">

  {%- include 'layouts/head/stylesheets' -%}

  <link rel="prefetch" as="script" id="main-worker" href="{{ 'autogenerated/sw.js' | asset_url }}">
{% endcontent_for %}


{% content_for 'body_bottom' %}
  <script>
    window.__WEBPACK_ASSETS_PATH__ = function() { return '{{ "" | asset_url }}autogenerated/' };
    window.__ASSETS_PATH__ = '{{ "" | asset_url }}';
  </script>

  <script src="{{ 'autogenerated/common.js' | asset_url }}" async></script>

  {%- for javascript_url in page.metadata.javascripts -%}
    <script src="{{ javascript_url | asset_url }}" async></script>
  {%- endfor -%}
{% endcontent_for %}
