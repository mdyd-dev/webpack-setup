{
  "name": "webpack-setup",
  "version": "0.0.1",
  "description": "Example of setting up PlatformOS project with webpack",
  "sideEffects": false,
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "sync": "marketplace-kit sync development",
    "build": "npm run build:development",
    "build:development": "webpack-cli --env production",
    "build:staging": "webpack-cli --env production",
    "build:production": "webpack-cli --env production",
    "lint-staged": "lint-staged",
    "deploy:development": "npm run build:development && marketplace-kit deploy development",
    "deploy:staging": "npm run build:staging && marketplace-kit deploy staging",
    "deploy:production": "npm run build:production && marketplace-kit deploy production"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mdyd-dev/webpack-setup.git"
  },
  "browserslist": [
    "last 2 versions",
    "iOS > 7",
    "Safari > 6",
    "IE > 10"
  ],
  "license": "MIT",
  "devDependencies": {
    "@platform-os/marketplace-kit": "^2.3.0",
